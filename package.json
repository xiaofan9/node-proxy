{
  "name": "node-proxy",
  "version": "1.0.1",
  "main": "app.js",
  "bin": "./bin/www.js",
  "author": "sifan <718662552@qq.com>",
  "description": "koa proxy service",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production&& set NODE_ENV=production && node ./bin/www",
    "start:pm2": "pm2 start ecosystem.config.js",
    "reload:pm2": "pm2 reload ecosystem.config.js",
    "dev": "cross-env NODE_ENV=development&& set NODE_ENV=development && nodemon ./bin/www",
    "debug": "cross-env debug=node-proxy:*&& set debug=node-proxy:* && npm run dev"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "crypto": "^1.0.1",
