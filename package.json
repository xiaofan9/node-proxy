    "koa-compress": "^5.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.2.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "mysql2": "^2.3.3",
    "nunjucks": "^3.2.3",
    "pm2": "^5.2.0",
    "portfinder": "^1.0.28",
    "sequelize": "^5.22.5",
    "v-axios": "^3.0.9"
  },
  "devDependencies": {
    "@vercel/node": "^1.15.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^6.2.0",
